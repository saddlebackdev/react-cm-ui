{
  "name": "react-cm-ui-docs",
  "version": "0.11.0",
  "scripts": {
    "build": "webpack",
    "build:clean": "rimraf build",
    "start": "webpack-dev-server --mode development --content-base docs/build/ --hot --progress --colors --host 0.0.0.0",
    "start:prod": "npm run build && webpack-dev-server --mode production --content-base docs/build/ --hot --progress --colors --host 0.0.0.0",
    "start:stats": "webpack --display-modules --display-chunks --json > stats.json --progress"
  },
  "dependencies": {
    "@babel/register": "^7.4.4",
    "classnames": "^2.2.6",
    "core-js": "^3.1.3",
    "lodash": "^4.17.19",
    "moment-timezone": "^0.5.31",
    "prop-types": "^15.7.2",
    "raw-loader": "^4.0.0",
    "react": "^16.8.6",
    "react-custom-scrollbars": "^4.0.0",
    "react-dom": "^16.13.1",
    "react-redux": "^7.1.1",
    "react-responsive": "^8.0.3",
    "react-router": "^3.2.4",
    "react-syntax-highlighter": "^11.0.2",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.2"
  },
  "devDependencies": {
    "@advclb/react-docgen-loader": "^0.1.0",
