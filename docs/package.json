    "node-sass": "~4.13.1",
    "postcss-loader": "^3.0.0",
    "react-docgen": "^5.3.0",
    "resolve-url-loader": "^3.1.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.13.1",
    "terser-webpack-plugin": "^4.2.2",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1"
  }
